{
  "name": "energi-explorer",
  "version": "1.7.6",
  "vcs": "git",
  "entryPoints": {
    "backend": {
      "tool": "node",
      "path": "bin/instance",
      "tune": {
        "internal": true,
        "minMemory": "128M"
      }
    },
    "frontend": {
      "tool": "nginx",
      "path": "public",
      "tune": {
        "socketPort": 3001
      }
    }
  },
  "webcfg": {
    "root": "public",
    "main": "backend",
    "mounts": {
      "/": {
        "app": "backend",
        "static": true,
        "tune": {
          "expires": "1h",
          "etag": true,
          "pattern": true,
          "staticGzip": true
        }
      },
      "/api/": "backend",
      "/ext/": "backend"
    }
  },
  "persistent": [
    "tmp"
  ]
}
